<h1> Log In </h1>

<%= flash[:message] if flash[:message] %>

<%= form_for(:user) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password%>
    <br>
    <br>

    <%= f.submit "Submit" %>
<% end %>
