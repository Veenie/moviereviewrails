<%= form_for(@review) do |f| %>
    <%= f.label "Movie" %>
    <br>
    <%= f.collection_select(:movie_id, Movie.all, :id, :title) %>
    <br>
    <%= f.label "Header" %>
    <br>
    <%= f.text_field :header %>
    <br>
    <%= f.label "Review" %>
    <br>
    <%= f.text_area :body, size: "30x10" %>
    <br>
    <%= f.submit %>
<% end %>    
